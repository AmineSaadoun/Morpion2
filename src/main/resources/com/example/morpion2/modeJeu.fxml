                    <font>
                        <Font name="Segoe UI Semibold" size="21.0" />
                    </font>
                </Label>
                <Button fx:id="EasyBtn" layoutX="160.0" layoutY="305.0" mnemonicParsing="false" onAction="#EasyBtnActionPerformed" prefHeight="25.0" prefWidth="103.0" text="Facile">
